{
  "name": "portfolio-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next,codegen -c blue.bold,green.bold \"next dev\" \"npm run codegen:watch\"",
    "build": "next build",
    "prebuild": "npm run codegen",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.ts -r dotenv/config",
    "codegen:watch": "npm run codegen -- --watch",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.7",
    "@next/font": "^13.1.6",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.3.0",
    "framer-motion": "^9.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "^13.1.6",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/client-preset": "^1.3.0",
    "@next/bundle-analyzer": "^13.1.6",
    "@types/node": "^18.11.18",
    "@types/ramda": "^0.28.22",
    "@types/react": "^18.0.27",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=16"
  }
}
