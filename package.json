    "@types/ramda": "^0.27.39",
    "@types/react": "^17.0.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
